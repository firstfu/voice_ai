{
    "expo": {
        "name": "智錄AI",
        "slug": "voice_ai",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./assets/images/notification-icon.png",
                    "color": "#3A7BFF",
                    "sounds": [
                        "./assets/sounds/notification.wav"
                    ]
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/b6994061-2631-45d6-90c2-71a8a9097db8"
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "b6994061-2631-45d6-90c2-71a8a9097db8"
            }
        }
    }
}